<h1>Listing time_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @time_types.each do |time_type| %>
  <tr>
    <td><%= time_type.name %></td>
    <td><%= link_to 'Show', time_type %></td>
    <td><%= link_to 'Edit', edit_time_type_path(time_type) %></td>
    <td><%= link_to 'Destroy', time_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

